{
  "version": 2,
  "name": "daylight-gan",
  "alias": "daylight-gan",
  "files": ["dist"],
  "github": {
    "enabled": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/js/(.*)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/js/$1"
    },
    {
      "src": "^/css/(.*)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/css/$1"
    },
    {
      "src": "^/img/(.*)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      },
      "dest": "/img/$1"
    },
    {
      "src": "^/favicon.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/robots.txt$",
      "dest": "/robots.txt"
    },
    {
      "src": ".*",
      "headers": {
        "cache-control": "max-age=0,must-revalidate"
      },
      "dest": "/index.html"
    }
  ]
}